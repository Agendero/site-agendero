{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Node.js",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/app.js",
            "console": "integratedTerminal",
            "restart": true,
            "autoAttachChildProcesses": true,
            "env": {
                "NODE_ENV": "development"
            }
        }
    ]
}
